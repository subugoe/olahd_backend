          "german_stemmer": {
            "type": "stemmer",
            "language": "light_german"
          },
          "english_stop": {
            "type": "stop",
            "stopwords": "_english_"
          },
          "english_stemmer": {
            "type": "stemmer",
            "language": "english"
          },
          "english_possessive_stemmer": {
            "type": "stemmer",
            "language": "possessive_english"
          }
        },
        "analyzer": {
          "rebuilt_german": {
            "tokenizer": "standard",
            "filter": [
              "lowercase",
              "german_stop",
              "german_normalization",
              "german_stemmer"
            ]
          },
          "rebuilt_english": {
            "tokenizer": "standard",
            "filter": [
              "english_possessive_stemmer",
              "lowercase",
              "english_stop",
              "english_stemmer"
            ]
          }
        }
      }
